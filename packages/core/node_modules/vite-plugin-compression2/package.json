{
  "name": "vite-plugin-compression2",
  "packageManager": "yarn@4.1.0",
  "version": "1.1.1",
  "description": "a fast vite compression plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "workspaces": [
    "example",
    "e2e/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "test": "c8 -r=lcov ava __tests__/*.spec.ts",
    "lint": "eslint . --fix",
    "e2e": "ava e2e/**/*.spec.ts"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nonzzz/vite-plugin-compression.git"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "compress"
  ],
  "author": "Kanno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nonzzz/vite-plugin-compression/issues"
  },
  "homepage": "https://github.com/nonzzz/vite-plugin-compression#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@nolyfill/es-aggregate-error": "^1.0.28",
    "@types/gunzip-maybe": "^1.4.2",
    "@types/node": "^17.0.14",
    "@types/tar": "^6.1.11",
    "@types/tar-stream": "^3.1.3",
    "ava": "^5.2.0",
    "c8": "^7.13.0",
    "eslint": "^8.40.0",
    "eslint-config-kagura": "^2.1.1",
    "playwright": "^1.32.3",
    "sirv": "^2.0.3",
    "tsup": "^7.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "gunzip-maybe": "^1.4.2",
    "tar-stream": "^3.1.7"
  },
  "ava": {
    "files": [
      "__tests__/*.spec.ts",
      "e2e/**/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "tsx/cjs"
    ]
  },
  "resolutions": {
    "function-bind": "npm:@nolyfill/function-bind@latest",
    "has": "npm:@nolyfill/has@latest"
  }
}
