#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/UI Library/ui-library/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.14.10/node_modules/@microsoft/api-extractor/bin/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.14.10/node_modules/@microsoft/api-extractor/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.14.10/node_modules/@microsoft/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.14.10/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/UI Library/ui-library/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.14.10/node_modules/@microsoft/api-extractor/bin/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.14.10/node_modules/@microsoft/api-extractor/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.14.10/node_modules/@microsoft/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/@microsoft+api-extractor@7.43.0_@types+node@20.14.10/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/api-extractor" "$@"
else
  exec node  "$basedir/../../bin/api-extractor" "$@"
fi
