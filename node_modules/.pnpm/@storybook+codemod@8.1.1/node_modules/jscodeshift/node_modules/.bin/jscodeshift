#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/UI Library/ui-library/node_modules/.pnpm/jscodeshift@0.15.2_@babel+preset-env@7.24.5_@babel+core@7.24.5_/node_modules/jscodeshift/bin/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/jscodeshift@0.15.2_@babel+preset-env@7.24.5_@babel+core@7.24.5_/node_modules/jscodeshift/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/jscodeshift@0.15.2_@babel+preset-env@7.24.5_@babel+core@7.24.5_/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/UI Library/ui-library/node_modules/.pnpm/jscodeshift@0.15.2_@babel+preset-env@7.24.5_@babel+core@7.24.5_/node_modules/jscodeshift/bin/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/jscodeshift@0.15.2_@babel+preset-env@7.24.5_@babel+core@7.24.5_/node_modules/jscodeshift/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/jscodeshift@0.15.2_@babel+preset-env@7.24.5_@babel+core@7.24.5_/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jscodeshift.js" "$@"
else
  exec node  "$basedir/../../bin/jscodeshift.js" "$@"
fi
