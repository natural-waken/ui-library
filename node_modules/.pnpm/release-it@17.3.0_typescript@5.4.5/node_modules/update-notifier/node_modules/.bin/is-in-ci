#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/UI Library/ui-library/node_modules/.pnpm/is-in-ci@0.1.0/node_modules/is-in-ci/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/is-in-ci@0.1.0/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/UI Library/ui-library/node_modules/.pnpm/is-in-ci@0.1.0/node_modules/is-in-ci/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/is-in-ci@0.1.0/node_modules:/mnt/d/UI Library/ui-library/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-in-ci@0.1.0/node_modules/is-in-ci/cli.js" "$@"
else
  exec node  "$basedir/../../../../../is-in-ci@0.1.0/node_modules/is-in-ci/cli.js" "$@"
fi
